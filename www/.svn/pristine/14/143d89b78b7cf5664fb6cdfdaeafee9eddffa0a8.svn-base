	<script>
		var topbar = $('#topbar');
		var topbarList = $('#topbar-list');
		$(document).ready(function () {
			$(window).scroll(function(event) {
				var scrollTop = $(window).scrollTop();
				if (scrollTop > 670) {
					topbar.addClass('bg');
					topbarList.addClass('bg');
				} else {
					topbar.removeClass('bg');
					topbarList.removeClass('bg');
				}
			});
		});
