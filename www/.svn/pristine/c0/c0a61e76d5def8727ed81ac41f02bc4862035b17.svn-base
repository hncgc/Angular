@charset "utf-8";
/*reset*/
body {margin: 0 auto;height: 100%;background: #fff;font-family: "Helvetica Neue", 'Helvetica', 'sans-serif', 'Microsoft Yahei', '微软雅黑', Arial, sans-serif;font-size: 14px;line-height: 135%;color: #494848; width: 100%;max-width: 1920px;min-width: 320px;-webkit-overflow-scrolling: touch;overflow-scrolling: touch;}
select,input[type="button"],input[type="submit"],input[type="reset"],input[type="text"],input[type="date"],input[type="password"] {padding: 0;margin: 0;background: #fff;-webkit-appearance: none;appearance: none;border-radius: 0}
section,header,footer,article,nav,aside,div,input,button,select,textarea,dl,dt,dd,h1,h2,h3,h4,h5,p,form,i,b,span,table,td,th,ol,ul,li,a,input,button,select {margin: 0;padding: 0;outline: none;-webkit-text-size-adjust: none;box-sizing: border-box;color: #666;/*font-family:'微软雅黑','黑体',Arial,Helvetica,sans-serif;*/-webkit-font-smoothing: antialiased;font-smoothing: antialiased}
b,i {font-style: normal;font-weight: normal}
table {border: 0;padding: 0;margin: 0;border-collapse: collapse;border-spacing: 0;}
h1,h2,h3,h4,h5,h6 {font-size: 16px;}
ul,ol,li {list-style: none;}
img {border: none;max-width: 100%;margin: 0 auto;}
input,img {vertical-align: middle;}
input{border:1px solid #e2e8ef;}
a{text-decoration: none;border: none;outline: none;color: #666}
section{margin:0px 20px;}
/*public*/
.fl {float: left;}
.fr {float: right}
.hide {display: none}
.show {display: block}
.inline-block {display: inline-block}
.textCenter{text-align:center;}
.clearfix:before,.clearfix:after {content: "";display: block;width: 0;height: 0;clear: both;overflow: hidden;}
.clearfix {*zoom: 1}
.clearfloat { clear :both;}
/*color*/
.blackColor{color:#023156;}
.grayColor{color:#737c82;}
.blueColor{color:#3A8FFF;}
/*公共样式*/
.blueButton{cursor: pointer; display:inline-block;background:#489cff;border-radius:@radius;width:100%;height:49px;line-height:49px;text-align:center;font-size:16px;color:#fff;border:none;}
.code.resend{background: #ccc}
.ellipsis {text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.flex {-webkit-box-flex: 1;-ms-flex: 1;display: block;}

@size:12px;
@radius:4px;
@grayColor:#737c82;
@blueColor:#3A8FFF;
@greenColor:#4cc243;
/*弹窗*/
.mask{display:none;height:100%;width:100%;background:rgba(0,0,0,0.4);position:fixed;z-index:3;}
/*底部*/

.box{display:-webkit-box;display:-ms-flexbox;z-index:10;}

footer,iframe{ position:fixed;bottom:0px;width:100%;max-width:640px;border-top:1px solid #eee;min-width: 320px}
footer .flex{-webkit-box-flex:1;-ms-flex:1;width:0%;text-align:center; background:#FFF;height:48px; overflow:hidden}
footer .active dd{color:#F44}
footer .flex dt{padding:7px 28px 0 28px}
footer .flex dd{ font-size:10px;color:#a8a8a8}
/*底部*/
#footer2{ position:fixed;bottom:0px;width:100%;max-width:640px;border-top:1px solid #eee;min-width: 320px;background: #fff}
#footer2 .flex{-webkit-box-flex:1;-ms-flex:1;width:0%;text-align:center; background:#fff;height:48px; overflow:hidden}
#footer2 .flex dt{padding:8px 28px 0 28px}
#footer2 .flex dd{ font-size:10px;padding-top:3px; color:#b6bcc6}
@import 'publicLESS.less';
/*公共的弹窗*/
.tips-bgc{
    position: fixed;
    z-index: 97;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.4);
}
.publicTip{
    .border-radius(4px);
    text-align: center;
    position: fixed;
    z-index: 98;
    width: 80%;
    left: 50%;
    margin-left: -40%;
    top: 50%;
    padding: 40px 20px;
    height: auto;
    background-color: #fff;
    border: solid 1px #CDCDCD;
    .publicTip-cont{
        .ft(15px);
        color: @black;
        line-height: 25px;
        .yellowColor{
            color: @yellow;
        }
    }
    .publicTip-close{
        margin-top: 30px;
        a{
            .btn;
            display: inline-block;
            width: 45%;
            max-width: 124px;
            .line-height(40px);
            &.publicTip-n{
                background-color: #E1E8F0;
                margin-left: 17px;
            }
            &.publicTip-s{
                width: 200px;
                max-width: 200px;
            }
        }
    }

}


